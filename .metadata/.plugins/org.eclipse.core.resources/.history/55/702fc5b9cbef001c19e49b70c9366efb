package lesson5Package;

public class W3 {

	public static void main(String[] args) {
		int a = 2;
		int b = 20;
		boolean n = true;
		
		for (int i = a; i < b & i > 1; i++) 
		{
			int result = i;
			for (int j = a; j < i; j++) 
			{
				if (i % j == 0) 
				{
					result = 0;
					break;
				}
			}
			
			if (result != 0) {
				System.out.println(result);
			}
		}

	}

}
